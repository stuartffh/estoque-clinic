{
  "name": "estoque-clinic-api",
  "version": "1.0.0",
  "description": "API REST para gestão de estoque de clínicas estéticas com multi-tenancy",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node scripts/init-db.js",
    "migrate": "node scripts/migrate.js",
    "migrate:status": "node scripts/migrate.js status",
    "migrate:rollback": "node scripts/migrate.js rollback",
    "backup": "node scripts/backup.js",
    "backup:restore": "node scripts/backup.js restore",
    "production:init": "node scripts/init-production.js",
    "production:validate": "node scripts/init-production.js validate",
    "production:health": "node scripts/init-production.js health",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "security:audit": "npm audit --audit-level moderate",
    "security:check": "npm audit --audit-level high"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dompurify": "^3.2.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "joi": "^18.0.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "redis": "^5.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.17.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.5",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.4"
  },
  "keywords": [
    "nodejs",
    "express",
    "jwt",
    "postgresql",
    "api",
    "estoque",
    "clinica",
    "estetica",
    "botox",
    "multi-tenancy",
    "inventory",
    "clinic"
  ],
  "author": "Codex",
  "license": "MIT"
}
